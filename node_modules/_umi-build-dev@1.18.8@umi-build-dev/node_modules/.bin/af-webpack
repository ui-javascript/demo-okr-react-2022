#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_af-webpack@1.14.9@af-webpack/bin/af-webpack.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_af-webpack@1.14.9@af-webpack/bin/af-webpack.js" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_umi-webpack-bundle-analyzer@3.6.2@umi-webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../_umi-webpack-bundle-analyzer@3.6.2@umi-webpack-bundle-analyzer/lib/bin/analyzer.js" "$@"
  ret=$?
fi
exit $ret

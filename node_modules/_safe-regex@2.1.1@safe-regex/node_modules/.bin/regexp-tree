#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../_regexp-tree@0.1.24@regexp-tree/bin/regexp-tree" "$@"
  ret=$?
else 
  node  "$basedir/../../../_regexp-tree@0.1.24@regexp-tree/bin/regexp-tree" "$@"
  ret=$?
fi
exit $ret

#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -x "$basedir/node" ]; then
  "$basedir/node"  "$basedir/../../../../_stylelint-config-prettier@5.3.0@stylelint-config-prettier/bin/check.js" "$@"
  ret=$?
else 
  node  "$basedir/../../../../_stylelint-config-prettier@5.3.0@stylelint-config-prettier/bin/check.js" "$@"
  ret=$?
fi
exit $ret
